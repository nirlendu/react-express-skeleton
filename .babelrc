{
	"presets": [
		[
			"@babel/preset-env",
			{
				"useBuiltIns": false
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-transform-runtime",
		[
			"@babel/plugin-proposal-decorators",
			{
				"decoratorsBeforeExport": true,
				"legacy": false
			}
		],
		[
			"module-resolver",
			{
				"root": [
					"."
				]
			}
		]
	]
}
